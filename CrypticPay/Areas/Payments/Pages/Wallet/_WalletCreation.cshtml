@using CrypticPay
@model CrypticPay.Globals.Status

@{
    if(Model == Globals.Status.Failure)
    {   
        <center>
            <p>There was an issue creating your wallet. Please contact support or try again later.</p>
        </center>
    }
    else
    {   
        <center>
            <p>Your Kryptik wallet has been created. You may now send crypto to anyone, anywhere at anytime!</p>
        </center>
    }
}
