@{

}
@model IQueryable<CrypticPay.Data.FileUpload>

<div class="row">
    @foreach (var upload in Model)
    {
        // create path from gateway provider and CID
        var imgPath = "https://cloudflare-ipfs.com/ipfs/" + upload.CID;
        <div class="col s12 l6">
            <img src="@imgPath" max-width="80%" />
        </div>
    }
</div>

