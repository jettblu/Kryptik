{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "hdkey@2.0.1",
      "destination": "wwwroot/lib/hdkey/"
    },
    {
      "library": "secrets.js-grempe@2.0.0",
      "destination": "wwwroot/lib/secrets.js-grempe/"
    },
    {
      "library": "requirejs@2.3.6",
      "destination": "wwwroot/lib/requirejs/"
    },
    {
      "library": "bip39@3.0.4",
      "destination": "wwwroot/bip39/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}